<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

   <ui:with field='resThemes' type='org.rstudio.core.client.theme.res.ThemeResources'/>
   <ui:with field="resLocal"
            type="org.rstudio.core.client.widget.ModalDialogBase2.Resources"/>

   <g:VerticalPanel ui:field="mainPanel_" styleName="{resLocal.styles.mainPanel}">
      <g:HTMLPanel ui:field="resizeLeft_" styleName="{resLocal.styles.resizeLeft}"></g:HTMLPanel>
      <g:HTMLPanel styleName="{resLocal.styles.resizeRight}"></g:HTMLPanel>
      <g:HTMLPanel styleName="{resLocal.styles.resizeTop}"></g:HTMLPanel>
      <g:HTMLPanel styleName="{resLocal.styles.resizeBottom}"></g:HTMLPanel>
      <g:HTMLPanel styleName="{resLocal.styles.resizeTopLeft}"></g:HTMLPanel>
      <g:HTMLPanel styleName="{resLocal.styles.resizeTopRight}"></g:HTMLPanel>
      <g:HTMLPanel styleName="{resLocal.styles.resizeBottomRight}"></g:HTMLPanel>
      <g:HTMLPanel styleName="{resLocal.styles.resizeBottomLeft}"></g:HTMLPanel>
      <g:HorizontalPanel
         ui:field="bottomPanel_"
         styleName="{resThemes.themeStyles.dialogBottomPanel}"
         width="100%">
         <g:HorizontalPanel ui:field="buttonPanel_">
            <g:HorizontalPanel ui:field="leftButtonPanel_">
            </g:HorizontalPanel>
         </g:HorizontalPanel>
      </g:HorizontalPanel>
   </g:VerticalPanel>

</ui:UiBinder>